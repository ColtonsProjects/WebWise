{
"manifest_version": 3,
"name": "Accessibility Extension",
"description": "Extension to improve accessibility on the web",
"version": "1.0.1",

  
"content_scripts": [
{
"matches": ["<all_urls>"],
"js": ["script.js"],
"css": ["style.css"]
}
],
  
"action": {
"default_popup": "index.html",
"default_icon": "icon.png"
},
  
"permissions": [
"activeTab",
"tabs",
"declarativeContent",
"storage",
"unlimitedStorage",
"downloads",
"scripting"
]
}